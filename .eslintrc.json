{
    "extends":[
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jx-a11y/recommended",
        "prettier"
    ],
    "rules": {
        "react/prop-type":0,
        "react/react-in-jsx-scope":0
     },
     "plugins":[
        "react",
        "import",
        "jxs-a11y"
     ],
    "parserOptions":{
        "ecmaVersion":"latest",
        "sourceType":"module",
        "ecmaFeatures":{
            "jsx":true
        }
    },
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },
    "settings":{
        "react":{
            "version":"detect"
        }
    },
    "import/resolver":{
        "node":{
            "extensions":[".jx",".jsx"]
        }
    }
}